{
  "name": "Send to Mom",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Send-to-mom button for Chrome. Created with extensionizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "src/options_custom/index.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Send to Mom",
    "default_popup": "src/popup/popup.html"
  },
  "permissions": [
    "tabs"
  ]
}